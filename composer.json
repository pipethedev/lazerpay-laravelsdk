{
    "name": "pipedev/lazerpay",
    "description": "laravel sdk for lazerpay",
    "license": "MIT",
    "keywords": [
        "php",
        "github",
        "laravel",
        "Open Source",
        "crypto",
        "defi",
        "lazerpay",
        "lazerpay.finance",
        "laravel 6",
        "laravel 7",
        "laravel 8"
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Muritala David",
            "email": "davmuri1414@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "../../../app/",
            "Pipedev\\Lazerpay\\": "src/",
            "Pipedev\\Lazerpay\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        
        "laravel/sanctum": "3.*",
        "laravel/sail": "1.13.3",
        "laravel/tinker": "2.*",
        "guzzlehttp/guzzle": "7.5.0",
        "fruitcake/laravel-cors": "3.0.0",
        "orchestra/testbench": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.2",
        "spatie/laravel-ignition": "^1.0"
    },
    "scripts" : {
        "test" : "./vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pipedev\\Lazerpay\\LazerPayServiceProvider"
            ],
            "aliases": {
                "Lazerpay": "Pipedev\\Lazerpay\\Facades\\LazerpayFacade"
            }
        }
    }
}
